        "s1_m70a": { "_id": "s1_m70a", "user": "3_unknown", "text": "}", "next": "s1_m71a" },
        "s1_m71a": { "_id": "s1_m71a", "user": "1_meera", "text": "It can see you.", "next": "s1_m72a" },
        "s1_m72a": { "_id": "s1_m72a", "user": "2_system", "text": "[END OF TRANSCRIPT]" },
        "s1_m66b": { "_id": "s1_m66b", "user": 0, "text": "No. I'm deleting it.", "next": "s1_m67b" },
        "s1_m67b": { "_id": "s1_m67b", "user": "2_system", "text": "File deleted.", "next": "s1_m68b" },
        "s1_m68b": { "_id": "s1_m68b", "user": 0, "text": "It's gone, Meera. You need help.", "next": "s1_m69b" },
        "s1_m69b": { "_id": "s1_m69b", "user": "1_meera", "text": "?", "next": "s1_m70b" },
        "s1_m70b": { "_id": "s1_m70b", "user": "1_meera", "text": "Json? What are you talking about?", "next": "s1_m71b" },
        "s1_m71b": { "_id": "s1_m71b", "user": "1_meera", "text": "I'm at the grocery store. I haven't texted you all day.", "next": "s1_m72b" },
        "s1_m72b": { "_id": "s1_m72b", "user": 0, "text": "What?", "next": "s1_m73b" },
        "s1_m73b": { "_id": "s1_m73b", "user": 0, "text": "But... the file... who sent me the file?", "next": "s1_m74b" },
        "s1_m74b": { "_id": "s1_m74b", "user": "2_system", "text": "You are now chatting with 'Observer'", "next": "s1_m75b" },
        "s1_m75b": { "_id": "s1_m75b", "user": "3_unknown", "text": "It doesn't matter who sent it.", "next": "s1_m76b" },
        "s1_m76b": { "_id": "s1_m76b", "user": "3_unknown", "text": "It only matters that you opened it.", "next": "s1_m77b" },
