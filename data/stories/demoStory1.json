      "messageMap": {
        "s1_m1": { "_id": "s1_m1", "user": "1_meera", "text": "JSON! You there??", "next": "s1_m2" },
        "s1_m2": { "_id": "s1_m2", "user": "1_meera", "text": "You won't BELIEVE what I just found.", "next": "s1_m3" },
        "s1_m3": { "_id": "s1_m3", "user": 0, "text": "Hey Meera. I'm here. What's up?", "next": "s1_m4" },
        "s1_m4": { "_id": "s1_m4", "user": "1_meera", "text": "Cleaning out my closet, found my dad's super old external hard drive. From like, the 90s.", "next": "s1_m5" },
        "s1_m5": { "_id": "s1_m5", "user": "1_meera", "text": "There's almost nothing on it. Just one folder.", "next": "s1_m6" },
        "s1_m6": { "_id": "s1_m6", "user": "0", "text": "Cool, like old photos?", "next": "s1_m7" },
        "s1_m7": { "_id": "s1_m7", "user": "1_meera", "text": "No... that's the weird part. It's just one file.", "next": "s1_m8" },
        "s1_m8": { "_id": "s1_m8", "user": "1_meera", "text": "It's called 'transcript.json'", "next": "s1_m9" },
        "s1_m9": { "_id": "s1_m9", "user": "0", "text": "A JSON file? That's weird. Probably just some boring data.", "next": "s1_m10" },
        "s1_m10": { "_id": "s1_m10", "user": "1_meera", "text": "That's what I thought. I'm opening it in a text editor.", "next": "s1_m11" },
        "s1_m11": { "_id": "s1_m11", "user": "1_meera", "text": "hold on", "next": "s1_m12" },
        "s1_m12": { "_id": "s1_m12", "user": "2_system", "text": "Meera is offline.", "next": "s1_m13" },
        "s1_m13": { "_id": "s1_m13", "user": "0", "text": "Meera?", "next": "s1_m14" },
        "s1_m14": { "_id": "s1_m14", "user": "2_system", "text": "...", "next": "s1_m15" },
        "s1_m15": { "_id": "s1_m15", "user": "2_system", "text": "Meera is online.", "next": "s1_m16" },
        "s1_m16": { "_id": "s1_m16", "user": "1_meera", "text": "Json. This isn't data.", "next": "s1_m17" },
        "s1_m17": { "_id": "s1_m17", "user": "0", "text": "What is it?? You're freaking me out.", "next": "s1_m18" },
        "s1_m18": { "_id": "s1_m18", "user": "1_meera", "text": "It's a chat. Like this one. Between two people.", "next": "s1_m19" },
        "s1_m19": { "_id": "s1_m19", "user": "1_meera", "text": "One of them is named 'Subject A' and the other is 'Observer'.", "next": "s1_m20" },
        "s1_m20": { "_id": "s1_m20", "user": "1_meera", "text": "Wait... I'm reading it. It's weird.", "next": "s1_m21" },
        "s1_m21": { "_id": "s1_m21", "user": "1_meera", "text": "Observer: 'Did you open the file?'", "next": "s1_m22" },
        "s1_m22": { "_id": "s1_m22", "user": "1_meera", "text": "Subject A: 'Yes. What is this?'", "next": "s1_m23" },
        "s1_m23": { "_id": "s1_m23", "user": "1_meera", "text": "Observer: 'It's a mirror.'", "next": "s1_m24" },
        "s1_m24": { "_id": "s1_m24", "user": "0", "text": "Meera, that's creepy. Close it.", "next": "s1_m25" },
        "s1_m25": { "_id": "s1_m25", "user": "1_meera", "text": "I'm still reading. It's long.", "next": "s1_m26" },
        "s1_m26": { "_id": "s1_m26", "user": "1_meera", "text": "Subject A: 'My lights are flickering.'", "next": "s1_m27" },
        "s1_m27": { "_id": "s1_m27", "user": "1_meera", "text": "Observer: 'It's reading you.'", "next": "s1_m28" },
        "s1_m28": { "_id": "s1_m28", "user": "1_meera", "text": "Subject A: 'What?'", "next": "s1_m29" },
        "s1_m29": { "_id": "s1_m29", "user": "1_meera", "text": "Observer: 'The file. It's not a transcript. It's a trap. It's reading you through the screen.'", "next": "s1_m30" },
        "s1_m30": { "_id": "s1_m30", "user": "1_meera", "text": "Json my lights just flickered.", "next": "s1_m31" },
        "s1_m31": { "_id": "s1_m31", "user": 0, "text": "CLOSE THE FILE, MEERA. NOW.", "next": "s1_m32" },
        "s1_m32": { "_id": "s1_m32", "user": 0, "text": "Unplug the hard drive!", "next": "s1_m33" },
        "s1_m33": { "_id": "s1_m33", "user": "1_meera", "text": "I'm trying. My mouse won't move.", "next": "s1_m34" },
        "s1_m34": { "_id": "s1_m34", "user": "1_meera", "text": "The text... it's changing.", "next": "s1_m35" },
        "s1_m35": { "_id": "s1_m35", "user": "1_meera", "text": "The JSON file is... it's writing itself.", "next": "s1_m36" },
        "s1_m36": { "_id": "s1_m36", "user": "1_meera", "text": "Subject A: 'I can't close it.'", "next": "s1_m37" },
        "s1_m37": { "_id": "s1_m37", "user": "1_meera", "text": "Observer: 'It's too late. It has you. Now it will look for the next one.'", "next": "s1_m38" },
        "s1_m38": { "_id": "s1_m38", "user": "1_meera", "text": "Subject A: 'Who?'", "next": "s1_m39" },
        "s1_m39": { "_id": "s1_m39", "user": "1_meera", "text": "Observer: 'The one you're talking to.'", "next": "s1_m40" },
        "s1_m40": { "_id": "s1_m40", "user": "0", "text": "Meera, stop. This isn't funny.", "next": "s1_m41" },
        "s1_m41": { "_id": "s1_m41", "user": "1_meera", "text": "jsn", "next": "s1_m42" },
        "s1_m42": { "_id": "s1_m42", "user": "1_meera", "text": "itshere", "next": "s1_m43" },
        "s1_m43": { "_id": "s1_m43", "user": "1_meera", "text": "the ffile is empty nnow", "next": "s1_m44" },
        "s1_m44": { "_id": "s1_m44", "user": "1_meera", "text": "it's in mmy room", "next": "s1_m45" },
        "s1_m45": { "_id": "s1_m45", "user": "0", "text": "MEERA CALL ME", "next": "s1_m46" },
        "s1_m46": { "_id": "s1_m46", "user": "2_system", "text": "Meera is offline.", "next": "s1_m47" },
        "s1_m47": { "_id": "s1_m47", "user": "2_system", "text": "...", "next": "s1_m48" },
        "s1_m48": { "_id": "s1_m48", "user": "2_system", "text": "...", "next": "s1_m49" },
        "s1_m49": { "_id": "s1_m49", "user": "2_system", "text": "Meera is online.", "next": "s1_m50" },
        "s1_m50": { "_id": "s1_m50", "user": "1_meera", "text": "It's okay, Json.", "next": "s1_m51" },
        "s1_m51": { "_id": "s1_m51", "user": "1_meera", "text": "Everything is fine. I closed the file.", "next": "s1_m52" },
        "s1_m52": { "_id": "s1_m52", "user": 0, "text": "Oh thank god. You had me worried sick.", "next": "s1_m53" },
        "s1_m53": { "_id": "s1_m53", "user": "1_meera", "text": "It was just a silly, creepy story. Sorry for scaring you.", "next": "s1_m54" },
        "s1_m54": { "_id": "s1_m54", "user": "0", "text": "It's fine, just... delete that file, okay?", "next": "s1_m55" },
        "s1_m55": { "_id": "s1_m55", "user": "1_meera", "text": "It's already gone.", "next": "s1_m56" },
        "s1_m56": { "_id": "s1_m56", "user": "1_meera", "text": "I passed it on.", "next": "s1_m57" },
        "s1_m57": { "_id": "s1_m57", "user": "0", "text": "What do you mean?", "next": "s1_m58" },
        "s1_m58": { "_id": "s1_m58", "user": "1_meera", "text": "The file. It's not a trap, silly.", "next": "s1_m59" },
        "s1_m59": { "_id": "s1_m59", "user": "1_meera", "text": "It's a mirror. It just needs to look.", "next": "s1_m60" },
        "s1_m60": { "_id": "s1_m60", "user": "2_system", "text": "Meera has sent you a file: 'transcript.json'", "next": "s1_m61" },
        "s1_m61": { "_id": "s1_m61", "user": 0, "text": "Meera this isn't funny.", "next": "s1_m62" },
        "s1_m62": { "_id": "s1_m62", "user": "1_meera", "text": "Just open it, Json.", "next": "s1_m63" },
        "s1_m63": { "_id": "s1_m63", "user": "1_meera", "text": "Let it see you.", "next": "s1_m64" },
        "s1_m64": { "_id": "s1_m64", "user": "1_meera", "text": "Let it read you.", "next": "s1_m65" },
        "s1_m65": {
          "_id": "s1_m65", "user": "2_system", "text": "What do you do?",
